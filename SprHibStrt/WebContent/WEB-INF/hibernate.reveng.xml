<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="lbr" match-name="country"/>
  <table-filter match-catalog="lbr" match-name="state"/>
  <table-filter match-catalog="lbr" match-name="district"/>
  <table-filter match-catalog="lbr" match-name="city"/>
  <table-filter match-catalog="lbr" match-name="locations"/>
  <table-filter match-catalog="lbr" match-name="userpermissions"/>
  
  <table-filter match-catalog="lbr" match-name="category"/>
  <table-filter match-catalog="lbr" match-name="events"/>
  <table-filter match-catalog="lbr" match-name="subcategory"/>
  <table-filter match-catalog="lbr" match-name="users"/>

  <table-filter match-catalog="lbr" match-name="missinglocations"/>
</hibernate-reverse-engineering>